{
  "name": "firebowyer",
  "version": "0.1.0",
  "description": "Angular 1x, Material Design, Webpack, ES6 and FireBase. Awesome and secure app in minutes.",
  "main": "index.js",
  "scripts": {
    "build": "npm_lifecycle_event=prod webpack --process --colors",
    "travis": "npm_lifecycle_event=prod webpack --process --colors",
    "start": "concurrently --kill-others 'webpack -w --process --colors' 'firebase serve'",
    "start-prod": "concurrently --kill-others 'npm_lifecycle_event=prod webpack -w --process --colors' 'firebase serve'"
  },
